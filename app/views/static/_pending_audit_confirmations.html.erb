<% pending_audit_confirmations.each do |pending_audit_confirmations| %>
  <%= link_to "I confirm that I did not perform any overtime for the week of: #{pending_audit_confirmations.start_date}", confirm_audit_log_path(pending_audit_confirmations), class: 'btn btn-primary btn-block btn-lg', id: "confirm_#{pending_audit_confirmations.id}", data: { confirm: 'Are you sure you want to confirm no overtime was performed?' } %>
<% end %>